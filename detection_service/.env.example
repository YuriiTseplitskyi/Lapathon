# Common Settings
AGENT_TYPE="openai" # or "lapa"
MODEL_TEMPERATURE="0.0"

# LAPA Settings (if AGENT_TYPE="lapa")
MODEL_NAME="lapa-function-calling"
LAPA_API_KEY="your-lapa-api-key"
BASE_URL="https://api.lapathoniia.top/"

# OpenAI Settings (if AGENT_TYPE="openai")
OPENAI_MODEL_NAME="gpt-4-turbo"
OPENAI_API_KEY="sk-..."

# Neo4j Settings
NEO4J_URI="bolt://localhost:7687"
NEO4J_USER="neo4j"
NEO4J_PASSWORD="password"
NEO4J_DATABASE="neo4j"
ALLOW_WRITE_QUERIES="false" # Set to "true" to allow the agent to write back to the graph

# LangSmith Tracing (Optional)
LANGSMITH_API_KEY="ls-..."
LANGSMITH_TRACING="true"
